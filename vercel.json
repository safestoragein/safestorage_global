{
  "rewrites": [
    { "source": "/uae/admin", "destination": "/index.html" },
    { "source": "/uae/blog", "destination": "/index.html" },
    { "source": "/uae/blog/:slug", "destination": "/index.html" },
    { "source": "/uae/blog/(.*)", "destination": "/index.html" },
    { "source": "/uae/(.*)", "destination": "/index.html" },
    { "source": "/uk", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}